/**
 * Dmultiplexor.  
 * {a,b} = {in,0} if sel==0
 *         {0,in} if sel==1
 */

CHIP DMux {
    IN in, sel;
    OUT a, b;

    PARTS:
    // Put your code here.
    Mux(a=in, b=0, sel=sel, out=a);
    And(a=in, b=sel, out=b);
}
